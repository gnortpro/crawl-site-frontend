{
  "parser": "typescript",
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "tabWidth": 2,
  "bracketSpacing": true,
  "bracketSameLine": true,
  "arrowParens": "avoid",
  "importOrder": [
    "^react/",
    "^next/",
    "^[base, model, theme, consts, hook, utils]",
    "^[component]",
    "^[./]"
  ],
  "importOrderSeparation": true
}